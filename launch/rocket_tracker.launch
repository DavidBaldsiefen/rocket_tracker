<launch>

    #Load rosparams
    <rosparam command="load" file="$(find rocket_tracker)/config/config.yaml" />
    
    #Starting framegrabber node
    <node 
        pkg="rocket_tracker" 
        type="frame_grabber_node" 
        name="FRAMEGRABBER"
        output="screen"/>

    #Starting image processor node
    <node 
        pkg="rocket_tracker" 
        type="image_processor_node" 
        name="IMAGEPROCESSOR"
        output="screen"/>

    #Starting evaluator node
    <node 
        pkg="rocket_tracker" 
        type="evaluator_node" 
        name="EVALUATOR"
        output="screen"/>
        
</launch>
